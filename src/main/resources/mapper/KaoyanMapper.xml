<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.english.mapper.KaoyanMapper">

    <select id="selectById" resultType="com.example.english.entity.Kaoyan">
        select *
        from kaoyan
        where id = #{id}
    </select>

</mapper>